{% extends 'base.html.twig' %}
{% import "common/helper.html.twig" as helper %}

{% block title %}{{ title|trans }} - {{ settings.webSite.siteName }}{% endblock %}
{% block content %}
    <section id="inner-headline">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <h2 class="pageTitle">{{ title|trans }}</h2>
                    {% include '/common/share.html.twig' %}
                </div>
            </div>
            <div class="row">
                {{ title|breadcrumbs|raw }}
            </div>
        </div>
    </section>
    <section id="content">
        <div class="container">
            <div class="row">
                <div class="col-lg-9">
                    <div id="app" class="col-xs-12"></div>

                <div class="col-lg-3">
                </div>
            </div>
        </div>
    </section>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset("dist/app/bundle.js") }}"></script>
{% endblock %}
