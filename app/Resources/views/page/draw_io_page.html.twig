<section id="inner-headline">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h2 class="pageTitle">{{ page.title }}</h2>
            </div>
        </div>
        <div class="row">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="{{ url('main_page') }}">{% trans %}Домой{% endtrans %}</a></li>
                <li class="breadcrumb-item active">{{ page.title }}</li>
            </ol>
        </div>
    </div>
</section>

<section id="content">
    <div class="container">
        <div class="row">
            {{ page.content|raw }}
        </div>
    </div>
</section>

<script type="text/javascript" src="https://www.draw.io/js/viewer.min.js"></script>