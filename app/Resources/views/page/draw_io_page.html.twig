<section id="inner-headline">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h2 class="pageTitle">{{ page.title }}</h2>
                {% include '/common/share.html.twig' %}
            </div>
        </div>
        <div class="row">
            {{ page|breadcrumbs|raw }}
        </div>
    </div>
</section>

<section id="content">
    <div class="container">
        <div class="row">
            {{ page.content|raw }}
        </div>
    </div>
</section>

